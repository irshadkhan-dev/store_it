else
  export NODE_PATH="/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/nitropack@2.10.4_drizzle-orm@0.39.2_@types+react@19.0.8_mysql2@3.12.0_react@19.0.0__mysql2@3.12.0_typescript@5.7.3/node_modules/nitropack/dist/cli/node_modules:/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/nitropack@2.10.4_drizzle-orm@0.39.2_@types+react@19.0.8_mysql2@3.12.0_react@19.0.0__mysql2@3.12.0_typescript@5.7.3/node_modules/nitropack/dist/node_modules:/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/nitropack@2.10.4_drizzle-orm@0.39.2_@types+react@19.0.8_mysql2@3.12.0_react@19.0.0__mysql2@3.12.0_typescript@5.7.3/node_modules/nitropack/node_modules:/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/nitropack@2.10.4_drizzle-orm@0.39.2_@types+react@19.0.8_mysql2@3.12.0_react@19.0.0__mysql2@3.12.0_typescript@5.7.3/node_modules:/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nitropack/dist/cli/index.mjs" "$@"
else
  exec node  "$basedir/../nitropack/dist/cli/index.mjs" "$@"
fi
