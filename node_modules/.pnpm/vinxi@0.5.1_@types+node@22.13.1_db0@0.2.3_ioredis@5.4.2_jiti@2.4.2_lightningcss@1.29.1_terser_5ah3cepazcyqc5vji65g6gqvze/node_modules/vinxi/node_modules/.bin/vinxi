#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/vinxi@0.5.1_@types+node@22.13.1_db0@0.2.3_ioredis@5.4.2_jiti@2.4.2_lightningcss@1.29.1_terser_5ah3cepazcyqc5vji65g6gqvze/node_modules/vinxi/bin/node_modules:/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/vinxi@0.5.1_@types+node@22.13.1_db0@0.2.3_ioredis@5.4.2_jiti@2.4.2_lightningcss@1.29.1_terser_5ah3cepazcyqc5vji65g6gqvze/node_modules/vinxi/node_modules:/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/vinxi@0.5.1_@types+node@22.13.1_db0@0.2.3_ioredis@5.4.2_jiti@2.4.2_lightningcss@1.29.1_terser_5ah3cepazcyqc5vji65g6gqvze/node_modules:/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/vinxi@0.5.1_@types+node@22.13.1_db0@0.2.3_ioredis@5.4.2_jiti@2.4.2_lightningcss@1.29.1_terser_5ah3cepazcyqc5vji65g6gqvze/node_modules/vinxi/bin/node_modules:/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/vinxi@0.5.1_@types+node@22.13.1_db0@0.2.3_ioredis@5.4.2_jiti@2.4.2_lightningcss@1.29.1_terser_5ah3cepazcyqc5vji65g6gqvze/node_modules/vinxi/node_modules:/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/vinxi@0.5.1_@types+node@22.13.1_db0@0.2.3_ioredis@5.4.2_jiti@2.4.2_lightningcss@1.29.1_terser_5ah3cepazcyqc5vji65g6gqvze/node_modules:/Users/irshadkhan.dev/tanstack/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.mjs" "$@"
else
  exec node  "$basedir/../../bin/cli.mjs" "$@"
fi
